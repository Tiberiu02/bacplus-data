		// Scene
		var scene = [];
		var scena_curenta = null;
		
		// Adaugare scena
		function adauga_scena(n, fn) {
			scene[n] = fn;
		}
		
		// Afisare scena
		function afiseaza_scena(n, parametri = null) {
			scena_curenta = n;
			scene[n](parametri);
