		/*/
		 * Scena 1
		 * Incarcare website
		/*/
		
		adauga_scena("incarcare", function() {
			
			// Afisare scena incarcare
			$("#incarcare").show();
			
			// Schimba la scena de prezentare
			setTimeout(function() {
				if(document.readyState === "complete") {
					afiseaza_scena("principal");
				} else {
					$(".se-incarca").html("așteptați câteva momente...");
					document.onreadystatechange = function (e) {
						if(document.readyState === "complete") {
							afiseaza_scena("principal");
						}
					}
				}
			}, 3000);
			
			// Procent incarcare
			var loading_procent = 15;
			var loading_interval = setInterval(function() {
				loading_procent += 15;
				if(loading_procent >= 100) {
					clearInterval(loading_interval);
					$("#incarcare .procent").html("100%");
				} else {
					$("#incarcare .procent").html(loading_procent + "%");
				}
			}, 300);
			
			// Animatii
						
		});
		

