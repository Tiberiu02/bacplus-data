		// Slideshow
		$(".slideshow").html('<div class="wrapper"><ul id="sb-slider" class="sb-slider">'
		+ '' + '</ul><div id="nav-dots" class="nav-dots">'
		+ ''
		+ '</div></div><div class="shadow"></div>');
		var done_slideshow = false;
		function do_slideshow() {
			if(done_slideshow == true) return;
			if($(".slideshow").length == 0) return;
			done_slideshow = true;
			var slideAuto = null;
			var Page = (function() {
				var $navDots = $( '#nav-dots' ).hide(),
					$shadow = $( '#shadow' ).hide(),
					$nav = $navDots.children( 'span' ),
					slicebox = $( '#sb-slider' ).slicebox( {
						onReady : function() {
							$navDots.show();
							$shadow.show();
						},
						orientation : 'v',
						cuboidsRandom : false,
						disperseFactor : 30,
						onBeforeChange : function( pos ) {
							if(!$(".slideshow .shadow").hasClass("animate")) $(".slideshow .shadow").addClass("animate");
							$(".slideshow .shadow").get(0).style.animation = 'none';
							$(".slideshow .shadow").get(0).offsetHeight;
							$(".slideshow .shadow").get(0).style.animation = null; 
							$nav.removeClass( 'nav-dot-current' );
							$nav.eq( pos ).addClass( 'nav-dot-current' );
						}
					}),
					init = function() {
						initEvents();
						setTimeout(function() {
							slicebox.next();
							slideAuto = setInterval(function() {
								slicebox.next();
							}, 4*1000);
						}, 1000);
					},
					initEvents = function() {
						$nav.each( function( i ) {
							$( this ).on( 'click', function( event ) {
								var $dot = $( this );
								if( !slicebox.isActive() ) {
									$nav.removeClass( 'nav-dot-current' );
									$dot.addClass( 'nav-dot-current' );
								}
								clearInterval(slideAuto);
								slicebox.jump( i + 1 );
								return false;
							} );
						} );
					};
					return { init : init };
			})();
			Page.init();
		}
		
