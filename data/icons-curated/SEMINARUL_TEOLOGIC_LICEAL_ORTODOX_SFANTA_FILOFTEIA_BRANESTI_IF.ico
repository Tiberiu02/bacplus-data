		// Deschide adresa url
		function openURL(url, pushHistory = true) {
			
			// Creare iFrame
			if(url != "" && !url.includes("embed=true")) {
				if(url.includes("?")) {
					url = url + "&embed=true";
				} else {
					url = url + "?embed=true";
				}
			}
			$("#cuprins .continut").html('<iframe src="' + url + '" style="border: 0; width: 100%; height: 100%; background-image: url(\'https://seminarulteologic.ro/templates/constructor/data/imagini/loading-falb.gif\'); background-size: auto; background-repeat: no-repeat; background-position: center center;" frameborder="0" scrolling="no"></iframe>');
			
			// Setare volum
			if(url.split("?")[0].endsWith("transmisiuni-in-direct/")) {
				$("#fundal").animate({volume: 0}, 1000);
			} else {
				$("#fundal").animate({volume: 1}, 1000);
			}
			
			// Schimbare adresa
			if(pushHistory == true) {
				if(url == "") {
					url = "https://seminarulteologic.ro";
				}
				url = url.replace("&embed=true", "");
				url = url.replace("embed=true", "");
				url = url.replace("?&", "?");
				url = url.trim();
				url = url.replace(/^\?+|\?+$/g, '');
				history.pushState({ path: url }, '', url);
			}
			
		} 
		
		// Apasare back si forward
		$(window).on("popstate", function (ev) {
			if(location.href.replace(/\/+$/,'') == "https://seminarulteologic.ro") {
				afiseaza_scena("principal");
				openURL("", false);
			} else {
				if(scena_curenta != "cuprins-tranzitie" && scena_curenta != "cuprins") {
					afiseaza_scena("cuprins");
				}
				gotoandplay(location.href, null, false);
			}
		});
		
		// Apasare buton		
		function gotoandplay(url, title = null, pushHistory = true) {
			
			// Completare titlu
			$(".titlu-scena").text(title);
						
			// Deschide adresa url
			openURL(url, pushHistory);
			
			// Afisare scena
			afiseaza_scena("cuprins-tranzitie");
			
		}

