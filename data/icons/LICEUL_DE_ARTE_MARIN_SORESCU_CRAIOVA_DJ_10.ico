		$(document).ready(function(){
			$('.closeAlertMsg').click(function(){
				$('.alertMsg').hide();
			})
			setTimeout(function(){ 
				$('.alertMsg').hide();
			}, 10000);
			
			function generateRnd(){
					
					var randomNr = Math.floor((Math.random() * 2) + 1) * Math.cos( Math.PI * Math.round( Math.random() ) );
					return randomNr;
			}
			
			
			$('.column-gal img').each(function(){
				
				$(this).css({
					"transform":  "skew(1deg) rotate("+generateRnd()+"deg)"
					
				})
				
			})
			
		})	
